<?xml version="1.0" encoding="utf-8"?>


<com.stepstone.stepper.StepperLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/stepperLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    app:ms_backButtonColor="@color/colorAccent"
    app:ms_completeButtonColor="@color/colorPrimary"
    app:ms_nextButtonColor="@color/colorAccent"
    app:ms_showErrorMessageEnabled="true"

    app:ms_stepperFeedbackType="content_progress|disabled_bottom_navigation"
    app:ms_stepperType="tabs"
    app:ms_tabNavigationEnabled="true"


    tools:context="com.erickogi14gmail.ishanu.Views.MainActivity"
    tools:showIn="@layout/activity_main" />


